<?xml version="1.0" encoding="utf-8" ?>
<syncConfiguration xmlns="http://grynwald.net/schemas/2014/ServerSync/v1/Configuration/">

  <actions>

    <compare enable="true"/>

    <export enable="true"  syncFolder="left" transferLocation="somepath" />

    <import enable="true"  syncFolder="left" transferLocation="somepath" />

    <copy enable="true" syncFolder="left"/>

    <readSyncState enable="true" fileName="someFile"/>

    <writeSyncState enable="true" fileName="someFile"/>

    <applyFilter enable="1" inputFilter="someFilter"/>

    <acquireLock enable="true" lockFile="someFile" />

    <releaseLock enable="true" lockFile="someFile"/>

    <sleep enable="true">
      <timeout m="2"/>
    </sleep>

  </actions>


</syncConfiguration>