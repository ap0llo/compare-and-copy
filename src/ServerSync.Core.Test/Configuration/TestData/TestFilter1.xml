<?xml version="1.0" encoding="utf-8" ?>
<syncConfiguration xmlns="http://grynwald.net/schemas/2014/ServerSync/v1/Configuration/">
	
	
	<!--Finds conflicts that aren't metadata files-->
	<filter name="Filter1">
		<include>
			<compareState value="Conflict" />
		</include>   
		<exclude>
			<regex pattern=".*\.ext1" />
			<regex pattern=".*\.ext2" />			
		</exclude>
	</filter>
		
	
</syncConfiguration>